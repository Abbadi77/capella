<plugin>
    <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.polarsys.capella.core.platform.sirius.ui.actions.Capella.page"
            class="org.polarsys.capella.core.transition.system.ui.preferences.TransitionLogPreferencePage"
            id="org.polarsys.capella.core.transition.system.ui.preferences.TransitionLogPreferencePage"
            name="Transition Log Preferences">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            category="org.polarsys.capella.core.platform.sirius.ui.actions.Capella.page"
            class="org.polarsys.capella.core.transition.system.ui.preferences.TransitionLogPreferencePage"
            id="org.polarsys.capella.core.transition.system.ui.preferences.TransitionLogPreferencePage"
            objectClass="org.eclipse.core.resources.IProject"
            name="Transition Log Preferences">
            <enabledWhen>
                 <reference definitionId="org.polarsys.capella.core.preferences.project.property.pages.visibilityExpression"/>
            </enabledWhen>
            
      </page>
   </extension>
   
   <extension
         point="org.polarsys.capella.common.flexibility.wizards">
         
      <propertyRenderer
            id="outputModelRenderer"
            class="org.polarsys.capella.core.transition.system.ui.preferences.OutputModelRenderer">
      </propertyRenderer>
      
   </extension>
   
   
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.polarsys.capella.core.transition.system.ui.preferences.TransitionsPreferencesInitializer">
      </initializer>
   </extension>
   
</plugin>
