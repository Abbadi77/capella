------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

// Open session and run the action Create REC wtih whole lib content
with [get-view "Capella Project Explorer" | get-tree] {
    select "lib/lib.aird" | double-click
    select "lib/lib.aird/lib" | get-menu -path "REC \\/ RPL/Create REC/With whole library content..." | click
}

// Verify that Actor and its part are present in the wizard
with [get-window "Replicable Elements" | get-group "" -index 1 | get-tree] {
    get-item -path "lib/lib/System Analysis/System Context/Actor 1 : Actor 1" | get-property caption 
        | equals "Actor 1 : Actor 1" | verify-true
    get-item -path "lib/lib/System Analysis/Actors/Actor 1" | get-property caption | equals "Actor 1" | verify-true
}
