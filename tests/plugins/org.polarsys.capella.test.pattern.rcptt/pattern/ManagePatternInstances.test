        | get-item "\\[LAB\\] Logical System - Logical Architecture Blank" -index 1] | double-click
}
with [get-editor "[LAB] Logical System - Logical Architecture Blank" | get-diagram -index 1] {
    with [get-edit-part -name "[LAB] Logical System - Logical Architecture Blank" | get-edit-part 
        -name "Logical System" | get-edit-part -className DNodeContainerViewNodeContainerCompartmentEditPart 
        | get-edit-part -name "Logical System" | get-edit-part 
        -className DNodeContainerViewNodeContainerCompartment2EditPart] {
