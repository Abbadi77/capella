get-view "Capella Project Explorer" | get-tree | select "patternProject/patternProject.patterns" 
    | get-menu "New/Capella Project" | click
with [get-window "New Capella Project"] {
    with [get-editbox] {
        set-text "cross-project"
    }
    get-button Finish | click
}
with [get-view "Capella Project Explorer" | get-tree] {
    select "cross-project/cross-project.aird/cross-project/Logical Architecture/Logical System" 
        | get-menu "New Diagram \\/ Table.../Logical Architecture Blank" | click
}
get-window "New Logical Architecture Blank" | get-button OK | click
with [get-editor "[LAB] Logical System" | get-diagram -index 1] {
    with [get-edit-part -name "[LAB] Logical System"] {
        mouse-move 154 135 button3 -height 535 -width 1268
        mouse-press 154 135 button3 -height 535 -width 1268
        mouse-release 154 135 button3 2097152 -height 535 -width 1268
    }
    get-menu "Patterns/Apply Pattern..." | click
}
with [get-window "Apply Pattern"] {
    get-button "Open..." | click
    get-window "Modeling Patterns" | get-button Cancel | click
    get-button Cancel | click
}
with [get-editor "[LAB] Logical System" | get-diagram -index 1] {
    with [get-edit-part -name "[LAB] Logical System"] {
        mouse-move 239 223 button3 -height 535 -width 1268
        mouse-press 239 223 button3 -height 535 -width 1268
        mouse-release 239 223 button3 2097152 -height 535 -width 1268
    }
    get-menu "Patterns/Apply Pattern..." | click
}
with [get-window "Apply Pattern"] {
    get-button "Open..." | click
    with [get-window "Modeling Patterns"] {
        get-tree | select "patternProject/patternProject.patterns"
        get-button OK | click
    }
    get-button "Next >" | click
    with [get-group Initialization] {
        get-editbox -after [get-label "Number of instances:"] | set-text 3
        get-button Propose | click
    }
    get-button Finish | click
}
get-window "Modeling Patterns" | get-button OK | click
get-window "Modeling Patterns" | get-button OK | click
get-window "Modeling Patterns" | get-button OK | click
get-editor "[LAB] Logical System" | get-diagram -index 1 | key-type "M2+F6"
with [get-window "Pattern Instance Management"] {
    with [get-group Representation] {
        get-button Highlight | click
        get-button Reset | click
    }
    get-button Close | click
}
with [get-editor "[LAB] Logical System" | get-diagram -index 1] {
    with [get-edit-part -name "[LAB] Logical System" | get-edit-part 
        -name "Logical System" | get-edit-part -className DNodeContainerViewNodeContainerCompartmentEditPart 
        | get-edit-part -name "Logical System_2"] {
        with [get-edit-part -className DNodeContainerViewNodeContainerCompartment2EditPart] {
            mouse-move 552 3 button1 -height 221 -width 561
            mouse-press 552 3 button1 -height 221 -width 561
            mouse-release 552 3 button1 524288 -height 221 -width 561
        }
        activate-direct-edit
        get-edit-part -className DNodeContainerName2EditPart | activate-direct-edit
        with [get-edit-part -className DNodeContainerViewNodeContainerCompartment2EditPart] {
            mouse-move 553 4 button3 -height 221 -width 561
            mouse-press 553 4 button3 -height 221 -width 561
        }
    }
    commit-direct-edit
    get-edit-part -name "[LAB] Logical System" | get-edit-part -name "Logical System" 
        | get-edit-part -className DNodeContainerViewNodeContainerCompartmentEditPart | get-edit-part 
        -name "Logical System_2" | get-edit-part -className DNodeContainerViewNodeContainerCompartment2EditPart 
        | mouse-release 553 4 button3 2097152 -height 221 -width 561
    get-menu "Patterns/Manage Instance(s)..." | click
}
with [get-window "Pattern Instance Management"] {
    get-group "Life-cycle" | get-button Delete | click
    get-window "Modeling Patterns" | get-button Yes | click
    get-group "Life-cycle" | get-button Delete | click
    with [get-window "Modeling Patterns"] {
        with [get-button "Keep instance elements"] {
            check
            uncheck
        }
        get-button Yes | click
    }
    get-button Close | click
}
